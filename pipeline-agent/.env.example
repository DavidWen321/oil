# ==========================================
# Pipeline Agent - Environment Variables
# ==========================================

# ===== LLM Configuration =====
# 通义千问 (推荐)
OPENAI_API_KEY=sk-your-api-key
OPENAI_API_BASE=https://dashscope.aliyuncs.com/compatible-mode/v1
LLM_MODEL=qwen-turbo
EMBEDDING_MODEL=text-embedding-v3

# 或使用OpenAI
# OPENAI_API_KEY=sk-your-openai-key
# OPENAI_API_BASE=https://api.openai.com/v1
# LLM_MODEL=gpt-4o-mini
# EMBEDDING_MODEL=text-embedding-3-small

# ===== Database Configuration =====
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=root
DB_NAME=pipeline_cloud

# ===== Redis Configuration =====
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# ===== Milvus Configuration =====
MILVUS_HOST=localhost
MILVUS_PORT=19530
MILVUS_COLLECTION=pipeline_knowledge

# ===== Java Service Configuration =====
JAVA_GATEWAY_URL=http://localhost:8080
JAVA_AUTH_USERNAME=admin
JAVA_AUTH_PASSWORD=admin123

# ===== Agent Configuration =====
AGENT_MAX_ITERATIONS=10
AGENT_TIMEOUT=60

# ===== RAG Configuration =====
RAG_CHUNK_SIZE=512
RAG_CHUNK_OVERLAP=50
RAG_TOP_K=10
RAG_FINAL_K=5
RAG_USE_HYDE=true
RAG_USE_RERANKING=true
RAG_USE_CONTEXTUAL=true

# ===== API Configuration =====
API_HOST=0.0.0.0
API_PORT=8100
API_DEBUG=true

# ===== Logging =====
LOG_LEVEL=INFO
LOG_FILE=logs/agent.log
